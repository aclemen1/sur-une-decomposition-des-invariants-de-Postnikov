B) \ar[r]^{\partial}\ar[d]_{\beta_*}  &\H_n (F) \ar[r]^{i_*} \ar[d]_{\phi_*}	&\H_n (E) \ar[r]^{p_*} \ar[d]_{\epsilon_*} &\H_n (B) \ar[d]_{\beta_*} \ar[r] &0\\ \H_{n+1}(E') \ar[r]_{p'_*} &\H_{n+1}(B') \ar[r]_{\partial'} &\H_n (F')	\ar[r]_{i'_*}	&\H_n (E') \ar[r]_{p'_*} &\H_n (B') \ar[r] &0.}$$\end{theorem}%*******************************************************************************\ifx\indiplome\undefined	\end{document}\fi\endinput